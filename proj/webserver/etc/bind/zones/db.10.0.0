;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	bomcliente.gsr. root.bomcliente.gsr. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;

	IN 	NS 	gsr.
	IN 	NS 	bomcliente.gsr.

26		IN 	PTR	gsr.
6		IN 	PTR	bomcliente.gsr.
2		IN 	PTR pc12.bomcliente.gsr.