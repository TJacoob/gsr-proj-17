;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	bomcliente.gsr. root.bomcliente.gsr. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;

@		IN 	NS 	ns.bomcliente.gsr.

6.0.0.10.in-addr.arpa.		IN 		PTR		ns.bomcliente.gsr.
2.0.0.10.in-addr.arpa.		IN 		PTR 	pc12.bomcliente.gsr.
