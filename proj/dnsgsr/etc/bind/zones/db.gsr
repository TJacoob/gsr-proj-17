;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	gsr. root.gsr. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;

	IN 	NS 	gsr.
	IN 	NS 	ns.bomcliente.gsr.
	IN 	NS 	ns1.contatudo.gsr.
	IN 	NS 	ns2.contatudo.gsr.

gsr. 					IN 	A 	10.0.0.26
ns.bomcliente.gsr. 		IN 	A 	10.0.0.6
ns1.contatudo.gsr. 		IN 	A 	10.0.2.98
ns2.contatudo.gsr. 		IN 	A 	10.0.2.99
